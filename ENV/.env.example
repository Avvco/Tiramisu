# Syntax: path_to_env_file_location entire_definition_of_your_env connection_symbol value
# With a space between each argument.

# Spring_Boot
# Do not modify this part ==========
./Spring-Boot/src/main/resources/application.properties server.forward-headers-strategy = framework
./Spring-Boot/src/main/resources/application.properties spring.devtools.restart.additional-paths = .
./Spring-Boot/src/main/resources/application.properties spring.devtools.restart.trigger-file = reload-trigger.txt
./Spring-Boot/src/main/resources/application.properties spring.devtools.restart.enabled = true
./Spring-Boot/src/main/resources/application.properties spring.devtools.livereload.enabled = true

./Spring-Boot/src/main/resources/application.properties spring.jpa.generate-ddl = true
./Spring-Boot/src/main/resources/application.properties spring.jpa.hibernate.ddl-auto = update
./Spring-Boot/src/main/resources/application.properties spring.datasource.url= jdbc:postgresql://spring-boot-postgres:5432/postgres
./Spring-Boot/src/main/resources/application.properties spring.datasource.username = postgres
./Spring-Boot/src/main/resources/application.properties spring.datasource.password = password
./Spring-Boot/src/main/resources/application.properties spring.task.scheduling.pool.size = 2
./Spring-Boot/src/main/resources/application.properties springdoc.show-actuator = true
./Spring-Boot/src/main/resources/application.properties springdoc.swagger-ui.path = /swagger-ui
./Spring-Boot/src/main/resources/application.properties management.endpoints.web.exposure.include = health
# Do not modify above ==========

# You can get your RPC entry point from https://rpc.info/ or any node provider you want.
./Spring-Boot/src/main/resources/application.properties blockchain.dev.contract.node-url = <useless-value>

# Remote deployed contract address
./Spring-Boot/src/main/resources/application.properties blockchain.dev.contract.address = <useless-value>

# Replace this private key with your Metamask account private key
# To export your private key from Metamask, open Metamask and
# go to Account Details > Export Private Key
# Be aware of NEVER putting real Ether into testing accounts
./Spring-Boot/src/main/resources/application.properties blockchain.dev.account.private-key = <useless-value>

# Hardhat
# For node url
# please set the goerli api url you get on Alchmey
# For Private key
# please set the private key in your wallet
./Hardhat/.env API_KEY = <useless-value>
./Hardhat/.env PRIVATE_KEY = <useless-value>