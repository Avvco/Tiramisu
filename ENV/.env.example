# Syntax: path_to_env_file_location key connection_symbol value
# With a space between each argument.

# Spring_Boot
# Do not modify this part ==========
./Spring-Boot/src/main/resources/application.properties spring.devtools.restart.additional-paths = .
./Spring-Boot/src/main/resources/application.properties spring.devtools.restart.trigger-file = reload-trigger.txt
./Spring-Boot/src/main/resources/application.properties spring.devtools.restart.enabled = true
./Spring-Boot/src/main/resources/application.properties spring.devtools.livereload.enabled = true

./Spring-Boot/src/main/resources/application.properties spring.jpa.generate-ddl = true
./Spring-Boot/src/main/resources/application.properties spring.jpa.hibernate.ddl-auto = create
./Spring-Boot/src/main/resources/application.properties spring.datasource.url = jdbc:mysql://spring-boot-mysql:3306/spring
./Spring-Boot/src/main/resources/application.properties spring.datasource.username = admin
./Spring-Boot/src/main/resources/application.properties spring.datasource.password = admin
./Spring-Boot/src/main/resources/application.properties spring.datasource.driver-class-name = com.mysql.cj.jdbc.Driver
# Do not modify above ==========

# You can get your RPC entry point from https://rpc.info/ or any node provider you want.
./Spring-Boot/src/main/resources/application.properties blockchain.dev.contract.node-url = 

# Remote deployed contract address
./Spring-Boot/src/main/resources/application.properties blockchain.dev.contract.address = 

# Replace this private key with your Metamask account private key
# To export your private key from Metamask, open Metamask and
# go to Account Details > Export Private Key
# Be aware of NEVER putting real Ether into testing accounts
./Spring-Boot/src/main/resources/application.properties blockchain.dev.account.private-key = 

